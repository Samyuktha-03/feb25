assign-1
n=int(input("n value"))
l=[]
positive=[]
negative=[]
for i in range (n):
    x=int(input())
    l.append(x)
for i in l:
    if i>0:
        positive.append(i)
    else:
        negative.append(i)
print(positive,' ',negative)

assign-2
n=int(input("enter n value"))
l=[]
t=[]
for x in range(n):
    x=int(input())
    l.append(x)
count=0
for i in range(n):
    i=int(i)
    if l[i]!=0:
        t.append(l[i])
        count+=1
print(t)
print("count",count)
